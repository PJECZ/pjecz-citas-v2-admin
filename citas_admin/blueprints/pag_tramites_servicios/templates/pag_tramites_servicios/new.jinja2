{% extends 'layouts/app.jinja2' %}
{% import 'macros/form.jinja2' as f with context %}
{% import 'macros/topbar.jinja2' as topbar %}

{% block title %}Nuevo Trámite y Servicio{% endblock %}

{% block topbar_actions %}
    {{ topbar.page('Nuevo Trámite y Servicio') }}
{% endblock %}

{% block content %}
    {% call f.card() %}
        {% call f.form_tag('pag_tramites_servicios.new', fid='pag_tramite_servicio_form') %}
            {% call f.form_group(form.nombre) %}{% endcall %}
            {% call f.form_group(form.costo) %}{% endcall %}
            {% call f.form_group(form.url) %}{% endcall %}
            {% call f.form_group(form.guardar) %}{% endcall %}
        {% endcall %}
    {% endcall %}
{% endblock %}
